    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "antd": "^4.23.1",
    "bignumber.js": "^9.1.0",
    "date-fns": "^2.29.2",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "tinymce": "^6.2.0"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.8.5"
