  ],
  "engines": {
    "node": ">=10.0",
    "npm": ">=5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zation/relient-admin.git"
  },
  "dependencies": {
    "@ant-design/icons": ">=4.2.1",
    "antd": ">=4.4.2",
    "bignumber.js": ">=9.0.0",
    "braft-editor": ">=2.3.9",
    "braft-utils": ">=3.0.12",
    "final-form": ">=4.20.1",
    "final-form-arrays": ">=3.0.2",
    "js-cookie": ">=2.2.1",
    "lodash": ">=4.17.19",
    "moment": ">=2.27.0",
    "prop-types": ">=15.7.2",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "react-final-form": ">=6.5.1",
    "react-final-form-arrays": ">=3.1.1",
    "react-final-form-listeners": ">=1.0.2",
    "react-images": ">=1.1.7",
    "react-qr-code": ">=1.0.2",
    "react-redux": ">=7.2.0",
    "redux": ">=4.0.5",
    "redux-actions": ">=2.6.5",
    "relient": ">=4.2.14",
    "universal-router": ">=9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-function-sent": "^7.10.4",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "less": "^3.12.2",
    "lint-staged": "^10.2.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore --ignore-pattern \"!**/.*\" .",
    "fix": "npm run lint --fix",
    "build": "rm -rf ./dist && mkdir ./dist && lessc src/styles.less dist/styles.css && cp ./package.json ./README.md ./dist/ && babel ./src --out-dir ./dist --source-maps inline",
    "pub": "npm run lint && npm run build && npm publish ./dist --registry=https://registry.npmjs.org/"
  }
}
