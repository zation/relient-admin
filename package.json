    "braft-editor": ">=2.3.9",
    "braft-utils": ">=3.0.12",
    "draft-js": ">=0.11.7",
    "draftjs-utils": ">=0.10.2",
    "js-cookie": ">=2.2.1",
    "lodash": ">=4.17.20",
    "moment": ">=2.29.1",
    "prop-types": ">=15.7.2",
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1",
    "react-images": ">=1.1.7",
    "react-qr-code": ">=1.0.5",
    "react-redux": ">=7.2.2",
    "redux": ">=4.0.5",
    "redux-actions": ">=2.6.5",
    "relient": "5.0.2",
    "universal-router": ">=9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@types/js-cookie": "^2.2.6",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "less": "^4.1.1",
    "lint-staged": "^10.5.3",
    "rc-select": "^12.1.9",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore --ignore-pattern \"!**/.*\" .",
    "fix": "npm run lint --fix",
    "build": "rm -rf ./dist && mkdir ./dist && lessc src/styles.less dist/styles.css && cp ./package.json ./README.md ./dist/ && tsc && babel --config-file ./.babelrc.js ./dist --out-dir ./dist",
    "pub": "npm run lint && npm run build && npm publish ./dist --registry=https://registry.npmjs.org/"
  }
}
