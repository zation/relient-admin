    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "antd": "^5.2.0",
    "bignumber.js": "^9.1.1",
    "date-fns": "^2.29.3",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "tinymce": "^6.3.1"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.8.5"
