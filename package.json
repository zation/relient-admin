  "engines": {
    "node": ">=10.0",
    "npm": ">=5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zation/relient-admin.git"
  },
  "dependencies": {
    "@ant-design/icons": ">=4.3.0",
    "@types/lodash": "^4.14.168",
