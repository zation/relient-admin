    "url": "git://github.com/zation/relient-admin.git"
  },
  "dependencies": {
    "@ant-design/icons": ">=4.3.0",
    "antd": ">=4.9.4",
    "bignumber.js": ">=9.0.1",
    "braft-convert": ">=2.3.0",
    "braft-editor": ">=2.3.9",
    "braft-utils": ">=3.0.12",
    "draft-js": ">=0.11.7",
    "draftjs-utils": ">=0.10.2",
    "final-form": ">=4.20.1",
    "final-form-arrays": ">=3.0.2",
    "js-cookie": ">=2.2.1",
    "lodash": ">=4.17.20",
    "moment": ">=2.29.1",
    "prop-types": ">=15.7.2",
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1",
    "react-final-form": ">=6.5.2",
    "react-final-form-arrays": ">=3.1.3",
    "react-final-form-listeners": ">=1.0.2",
    "react-images": ">=1.1.7",
    "react-qr-code": ">=1.0.5",
    "react-redux": ">=7.2.2",
    "redux": ">=4.0.5",
    "redux-actions": ">=2.6.5",
    "relient": ">=4.4.1",
    "universal-router": ">=9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-function-sent": "^7.12.1",
    "@babel/plugin-proposal-numeric-separator": "^7.12.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "less": "^4.0.0",
    "lint-staged": "^10.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore --ignore-pattern \"!**/.*\" .",
    "fix": "npm run lint --fix",
    "build": "rm -rf ./dist && mkdir ./dist && lessc src/styles.less dist/styles.css && cp ./package.json ./README.md ./dist/ && babel ./src --out-dir ./dist --source-maps inline",
    "pub": "npm run lint && npm run build && npm publish ./dist --registry=https://registry.npmjs.org/"
  }
}
